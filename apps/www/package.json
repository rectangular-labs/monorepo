{
  "name": "www",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "pnpm with-env-local vite",
    "build:preview": "pnpm with-env-preview vite build && tsc",
    "build:production": "pnpm with-env-prod vite build && tsc",
    "serve:preview": "pnpm with-env-preview vite preview",
    "serve:production": "pnpm with-env-prod vite preview",
    "test": "vitest run",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "format": "pnpx @biomejs/biome format . --write",
    "lint": "pnpx @biomejs/biome lint . --write",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env-local": "dotenvx run -f ../../.env.local  -f ../../.env -- ",
    "with-env-preview": "dotenvx run  -f ../../.env -- ",
    "with-env-prod": "dotenvx run -f ../../.env.production -- "
  },
  "dependencies": {
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "@rectangular-labs/api": "workspace:*",
    "@rectangular-labs/auth": "workspace:*",
    "@rectangular-labs/content": "workspace:*",
    "@rectangular-labs/ui": "workspace:*",
    "@simplewebauthn/browser": "^13.1.2",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-devtools": "^5.87.1",
    "@tanstack/react-router": "^1.131.35",
    "@tanstack/react-router-devtools": "^1.131.35",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.131.35",
    "arktype": "^2.1.22",
    "motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@rectangular-labs/typescript": "workspace:*",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^5.0.2",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  }
}
