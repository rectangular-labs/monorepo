{
  "name": "www",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "dotenvx decrypt -f ../../.env --stdout > .env.local && cat ../../.env.local >> .env.local && pnpm cf-typegen && pnpm with-env-local vite",
    "build:local": "pnpm with-env-local vite build",
    "build:preview": "pnpm with-env-preview vite build ",
    "build:production": "pnpm with-env-production vite build",
    "serve:local": "pnpm with-env-local vite preview",
    "test": "vitest run",
    "clean": "git clean -xdf .turbo node_modules dist .cache .wrangler",
    "format": "pnpx @biomejs/biome format . --write",
    "lint": "pnpx @biomejs/biome lint . --write",
    "typecheck": "pnpm cf-typegen && pnpm tsc --noEmit --emitDeclarationOnly false",
    "with-env-local": "dotenvx run  -f ../../.env.local -f ../../.env -- ",
    "with-env-preview": "dotenvx run  -f ../../.env -- ",
    "with-env-production": "dotenvx run  -f ../../.env.production -- ",
    "wrangler": "wrangler",
    "cf-typegen": "wrangler types --env-interface Env"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@rectangular-labs/content": "workspace:*",
    "@rectangular-labs/ui": "workspace:*",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.133.27",
    "@tanstack/react-router-devtools": "^1.133.27",
    "@tanstack/react-router-ssr-query": "^1.133.27",
    "@tanstack/react-start": "^1.133.27",
    "arktype": "^2.1.23",
    "motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.15",
    "@rectangular-labs/typescript": "workspace:*",
    "@tailwindcss/vite": "^4.1.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^5.1.0",
    "jiti": "^2.6.1",
    "jsdom": "^26.1.0",
    "typescript": "^5.9.3",
    "vite": "7.1.12",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.45.0"
  }
}
