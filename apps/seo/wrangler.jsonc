{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "rectangular-labs-seo",
  "main": ".output/server/index.mjs",
  "preview_urls": false,
  "compatibility_date": "2025-09-07",
  "compatibility_flags": ["nodejs_compat", "enable_nodejs_http_modules"],
  "assets": {
    "directory": ".output/public"
  },
  "observability": {
    "enabled": true
  },
  "placement": {
    "mode": "smart"
  },
  // mostly a workaround to avoid issues with the build and next themes
  "keep_names": false,
  "env": {
    "production": {
      "routes": [
        {
          "pattern": "seo.rectangularlabs.com",
          "custom_domain": true
        }
      ]
    },
    "{env.PULL_REQUEST}": {
      "routes": [
        {
          "pattern": "{env.PULL_REQUEST}.seo.rectangularlabs.com",
          "custom_domain": true
        }
      ]
    }
  }
}
