{
  "name": "seo-contact",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "dotenvx decrypt -f ../../.env --stdout > .env.local && node -e \"const fs=require('node:fs'); const p='../../.env.local'; if (fs.existsSync(p)) fs.appendFileSync('.env.local', fs.readFileSync(p));\" && pnpm cf-typegen && pnpm with-env-local vite",
    "build:local": "pnpm with-env-local vite build",
    "build:preview": "pnpm with-env-preview vite build",
    "build:production": "pnpm with-env-production vite build",
    "serve:local": "pnpm with-env-local vite preview",
    "test": "vitest run",
    "clean": "git clean -xdf .turbo node_modules dist .cache .wrangler",
    "format": "pnpx @biomejs/biome format . --write",
    "lint": "pnpx @biomejs/biome lint . --write",
    "typecheck": "pnpm cf-typegen && pnpm tsc --noEmit --emitDeclarationOnly false",
    "with-env-local": "dotenvx run  -f ../../.env.local -f ../../.env -- ",
    "with-env-preview": "dotenvx run  -f ../../.env -- ",
    "with-env-production": "dotenvx run  -f ../../.env.production -- ",
    "wrangler": "wrangler",
    "cf-typegen": "wrangler types --env-interface Env"
  },
  "dependencies": {
    "@rectangular-labs/ui": "workspace:*",
    "@t3-oss/env-core": "^0.13.10",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router": "^1.162.1",
    "@tanstack/react-router-devtools": "^1.162.1",
    "@tanstack/react-router-ssr-query": "^1.162.1",
    "@tanstack/react-start": "^1.162.1",
    "arktype": "^2.1.29",
    "motion": "^12.34.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tanstack-router-sitemap": "^1.0.13"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.25.2",
    "@rectangular-labs/typescript": "workspace:*",
    "@tailwindcss/vite": "^4.2.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "jiti": "^2.6.1",
    "jsdom": "^26.1.0",
    "miniflare": "4.20260219.0",
    "typescript": "^5.9.3",
    "vite": "7.3.1",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.18",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.67.0"
  }
}
