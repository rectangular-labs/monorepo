{
  "name": "@rectangular-labs/api-seo",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./src/client.ts"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./src/server.ts"
    },
    "./websocket-server": {
      "types": "./dist/server.websocket.d.ts",
      "default": "./src/server.websocket.ts"
    },
    "./context": {
      "types": "./dist/context.d.ts",
      "default": "./src/context.ts"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "default": "./src/env.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./src/types.ts"
    },
    "./workflows": {
      "types": "./dist/workflows/index.d.ts",
      "default": "./src/workflows/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc && tsx ./src/_open-api/open-api-resolver.ts",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "format": "pnpx @biomejs/biome format . --write",
    "lint": "pnpx @biomejs/biome lint . --write",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251121.0",
    "@rectangular-labs/typescript": "workspace:*",
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.56",
    "@ai-sdk/google": "^2.0.46",
    "@ai-sdk/openai": "^2.0.85",
    "@jimp/core": "^1.6.0",
    "@jimp/wasm-webp": "^1.6.0",
    "@orpc/client": "^1.11.3",
    "@orpc/contract": "^1.11.3",
    "@orpc/server": "^1.11.3",
    "@orpc/tanstack-query": "^1.11.3",
    "@rectangular-labs/api-core": "workspace:*",
    "@rectangular-labs/auth": "workspace:*",
    "@rectangular-labs/core": "workspace:*",
    "@rectangular-labs/dataforseo": "workspace:*",
    "@rectangular-labs/db": "workspace:*",
    "@rectangular-labs/google-apis": "workspace:*",
    "@rectangular-labs/loro-file-system": "workspace:*",
    "@rectangular-labs/result": "workspace:*",
    "@rectangular-labs/task": "workspace:*",
    "@t3-oss/env-core": "^0.13.8",
    "ai": "^5.0.112",
    "arktype": "^2.1.27",
    "aws4fetch": "^1.0.20",
    "cloudflare": "^5.2.0",
    "jimp": "^1.6.0"
  }
}
