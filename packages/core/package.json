{
  "name": "@rectangular-labs/core",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
