    "lint": "bun x @biomejs/biome lint . --write",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@rectangular-labs/typescript": "workspace:*",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "@rectangular-labs/result": "workspace:*",
    "@shopify/admin-api-client": "^1.1.1",
    "arktype": "^2.1.29"
  }
}
