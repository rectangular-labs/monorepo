{
  "name": "@rectangular-labs/api-core",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./src/lib/*.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "format": "bun x @biomejs/biome format . --write",
    "lint": "bun x @biomejs/biome lint . --write",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@rectangular-labs/typescript": "workspace:*",
    "@types/node": "^24.10.13",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@orpc/arktype": "^1.13.5",
    "@orpc/client": "^1.13.5",
    "@orpc/contract": "^1.13.5",
    "@orpc/json-schema": "^1.13.5",
    "@orpc/openapi": "^1.13.5",
    "@orpc/openapi-client": "^1.13.5",
    "@orpc/server": "^1.13.5",
    "@rectangular-labs/auth": "workspace:*",
    "arktype": "^2.1.29"
  }
}
