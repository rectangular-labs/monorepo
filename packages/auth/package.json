{
  "name": "@rectangular-labs/auth",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/server.ts"
    },
    "./client": {
      "default": "./src/client.ts"
    },
    "./components/*": {
      "default": "./src/components/*.tsx"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc",
    "generate": "pnpm with-env pnpx @better-auth/cli generate --config ./src/generate.ts --output ../db/src/schema/auth-schema.ts",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "format": "pnpx @biomejs/biome format . --write",
    "lint": "pnpx @biomejs/biome lint . --write",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenvx run -f ../../.env.local  -f ../../.env -- "
  },
  "devDependencies": {
    "@rectangular-labs/typescript": "workspace:*",
    "@tanstack/react-query": "^5.90.2",
    "@types/node": "^24.6.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "class-variance-authority": "^0.7.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.90.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "dependencies": {
    "@better-auth/expo": "1.4.0-beta.10",
    "@rectangular-labs/emails": "workspace:*",
    "@rectangular-labs/ui": "workspace:*",
    "@t3-oss/env-core": "^0.13.8",
    "arktype": "^2.1.22",
    "better-auth": "1.4.0-beta.10",
    "lucide-react": "^0.544.0"
  }
}
