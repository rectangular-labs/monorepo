{
  "name": "@rectangular-labs/content",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "./styles.css": "./src/styles.css",
    ".": {
      "types": "./dist/src/source.d.ts",
      "default": "./src/source.ts"
    },
    "./search": {
      "types": "./dist/src/lib/search.d.ts",
      "default": "./src/lib/search.ts"
    },
    "./rss": {
      "types": "./dist/src/lib/rss.d.ts",
      "default": "./src/lib/rss.ts"
    },
    "./get-posts-overview": {
      "types": "./dist/src/lib/get-posts-overview.d.ts",
      "default": "./src/lib/get-posts-overview.ts"
    },
    "./collections": "./.content-collections/generated/index.ts",
    "./ui/*": {
      "types": "./dist/src/components/*.d.tsx",
      "default": "./src/components/*.tsx"
    }
  },
  "scripts": {
    "build": "content-collections build && tsc",
    "dev": "concurrently \"content-collections build\" \"tsc\"",
    "dev:watch": "concurrently \"content-collections watch\" \"tsc --watch\"",
    "clean": "git clean -xdf .turbo node_modules dist .cache .next .content-collections",
    "format": "bun x @biomejs/biome format . --write",
    "lint": "bun x @biomejs/biome lint . --write",
    "typecheck": "pnpm run build && tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@content-collections/cli": "^0.1.9",
    "@content-collections/core": "^0.14.1",
    "@content-collections/mdx": "^0.2.2",
    "@fumadocs/content-collections": "^1.2.6",
    "@rectangular-labs/typescript": "workspace:*",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "fumadocs-typescript": "^4.0.14",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "reading-time": "^1.5.0",
    "rehype-external-links": "^3.0.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "dependencies": {
    "@rectangular-labs/ui": "workspace:*",
    "arktype": "^2.1.29",
    "feed": "^5.2.0",
    "fumadocs-core": "^15.8.5",
    "fumadocs-ui": "^15.8.5",
    "lucide-static": "^0.575.0"
  }
}
