{
  "name": "@rectangular-labs/task",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "./env": {
      "default": "./src/env.ts",
      "types": "./dist/src/env.d.ts"
    },
    "./client": {
      "default": "./src/client.ts",
      "types": "./dist/client.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "deploy": "pnpx trigger.dev@4.1.1 deploy",
    "dev:trigger": "pnpm dotenvx run -f ../../.env.local -f ../../.env -- pnpx trigger.dev@4.1.1 dev",
    "clean": "git clean -xdf .turbo node_modules dist .cache .trigger storage",
    "format": "bun x @biomejs/biome format . --write",
    "lint": "bun x @biomejs/biome lint . --write",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@rectangular-labs/typescript": "workspace:*",
    "@trigger.dev/build": "4.4.1",
    "@types/node": "^24.10.13",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.30",
    "@ai-sdk/openai": "^3.0.30",
    "@orama/orama": "^3.1.18",
    "@rectangular-labs/core": "workspace:*",
    "@rectangular-labs/dataforseo": "workspace:*",
    "@rectangular-labs/db": "workspace:*",
    "@rectangular-labs/result": "workspace:*",
    "@t3-oss/env-core": "^0.13.10",
    "@trigger.dev/sdk": "4.4.1",
    "ai": "^6.0.97",
    "arktype": "^2.1.29",
    "crawlee": "^3.16.0",
    "defuddle": "^0.7.0",
    "jsdom": "^26.1.0",
    "playwright": "1.58.2"
  }
}
