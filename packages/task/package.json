{
  "name": "@rectangular-labs/task",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "./env": {
      "default": "./src/env.ts",
      "types": "./dist/src/env.d.ts"
    },
    "./client": {
      "default": "./src/client.ts",
      "types": "./dist/client.d.ts"
    },
    "./schema/*": {
      "default": "./src/schema/*.ts",
      "types": "./dist/schema/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev:trigger": "pnpm dotenvx run -f ../../.env.local -f ../../.env -- pnpx trigger.dev@4.0.2 dev",
    "clean": "git clean -xdf .turbo node_modules dist .cache .trigger storage",
    "format": "bun x @biomejs/biome format . --write",
    "lint": "bun x @biomejs/biome lint . --write",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@rectangular-labs/typescript": "workspace:*",
    "@trigger.dev/build": "4.0.2",
    "@types/node": "^24.5.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@orama/orama": "^3.1.14",
    "@t3-oss/env-core": "^0.13.8",
    "@trigger.dev/sdk": "4.0.2",
    "arktype": "^2.1.22",
    "crawlee": "^3.14.1",
    "defuddle": "^0.6.6",
    "jsdom": "^26.1.0",
    "playwright": "1.55.0",
    "unstorage": "^1.17.1"
  }
}
