{
  "name": "@rectangular-labs/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/client.d.ts",
      "default": "./src/client.ts"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "default": "./src/env.ts"
    },
    "./parsers": {
      "types": "./dist/schema-parsers/index.d.ts",
      "default": "./src/schema-parsers/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "migrate-generate": "pnpm with-env-local drizzle-kit generate",
    "migrate-push:local": "pnpm with-env-local drizzle-kit migrate",
    "migrate-push:preview": "pnpm with-env-preview drizzle-kit migrate",
    "migrate-push:production": "pnpm with-env-production drizzle-kit migrate",
    "push": "pnpm with-env-local drizzle-kit push",
    "studio": "pnpm with-env-local drizzle-kit studio",
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "format": "pnpx @biomejs/biome format . --write",
    "lint": "pnpx @biomejs/biome lint . --write",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env-local": "dotenvx run -f ../../.env.local  -f ../../.env -- ",
    "with-env-preview": "dotenvx run -f ../../.env -- ",
    "with-env-production": "dotenvx run -f ../../.env.production -- "
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "arktype": "^2.1.22",
    "drizzle-arktype": "^0.1.3",
    "drizzle-orm": "^0.44.5",
    "postgres": "^3.4.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@rectangular-labs/typescript": "workspace:*",
    "@types/node": "^24.5.2",
    "drizzle-kit": "^0.31.4",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "drizzle-orm": "^0.44.5"
  }
}
