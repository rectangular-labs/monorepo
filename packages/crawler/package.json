{
  "name": "@rectangular-labs/crawler",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./env": {
      "types": "./dist/src/env.d.ts",
      "default": "./src/env.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "format": "bun x @biomejs/biome format . --write",
    "lint": "bun x @biomejs/biome lint . --write",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@rectangular-labs/typescript": "workspace:*",
    "@types/node": "^24.3.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "arktype": "^2.1.22",
    "crawlee": "^3.14.1",
    "playwright": "^1.55.0"
  }
}
